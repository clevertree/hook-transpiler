#!/usr/bin/env sh
##############################################################################
##
##  Gradle start up script for UN*X
##
##############################################################################
APP_NAME="Gradle"
APP_HOME="$(cd "$(dirname "$0")" && pwd -P)"
exec "$JAVA_HOME/bin/java" -cp "$APP_HOME/gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
